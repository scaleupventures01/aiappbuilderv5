{
  "name": "elite-trading-coach-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "start": "node server.js",
    "start:dev": "NODE_ENV=development node server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:api": "node test-api.js",
    "e2e": "playwright test",
    "format": "prettier . --write",
    "security:scan": "npm audit --json",
    "test:production": "node test-production-mode.mjs",
    "test:production:verbose": "node test-production-mode.mjs --verbose",
    "validate:production": "./scripts/validate-production-deployment.sh",
    "check:roadmap": "node ../orch/lib/check-roadmap-sync.mjs",
    "build:roadmap": "node ../orch/lib/generate-roadmap-html.mjs"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.5.1",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.0",
    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.51.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "sharp": "^0.34.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "canvas": "^3.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.16.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^3.2.4"
  }
}
