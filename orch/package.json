{
  "name": "orch",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Orchestration system for product development lifecycle",
  "scripts": {
    "start": "node lib/orch-start-real.mjs",
    "start:real": "node lib/orch-start-real.mjs",
    "start:old": "node lib/orch-start.mjs",
    "start:fast": "node lib/orch-start-optimized.mjs",
    "start:auth": "node auth/server.mjs",
    "scaffold": "node lib/scaffold-feature.mjs",
    "decompose": "node lib/task-decomposer.mjs",
    "check": "node lib/check-roadmap-sync.mjs && node lib/check-size.mjs",
    "task:breakdown": "node lib/task-decomposer.mjs",
    "task:assign": "node scripts/role-assign.mjs",
    "task:status": "node lib/orch/task-manager.mjs status",
    "task:update": "node lib/orch/task-manager.mjs update",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:auth": "node auth/tests/auth.test.mjs",
    "validate": "node scripts/validate-improvements.mjs",
    "health": "node scripts/health-check.mjs",
    "test:integration": "node tests/integration/full-workflow.test.mjs",
    "test:full": "npm run validate && npm run health && npm run test:integration",
    "manual": "node lib/orch-start-real.mjs --manual",
    "manual:old": "node lib/orch-start.mjs --manual",
    "manual:fast": "node lib/orch-start-optimized.mjs --manual",
    "report": "node lib/orch-start.mjs --generate-report",
    "report:fast": "node lib/orch-start-optimized.mjs --generate-report",
    "agents": "node lib/orch-start-real.mjs",
    "agents:real": "node lib/orch-start-real.mjs",
    "agents:test": "node test-real-agents.mjs",
    "agents:example": "node -e \"import('./lib/orch/task-tool-integration.mjs').then(m => m.exampleRealAgentUsage())\"",
    "agents:list": "node lib/orch-agents.mjs list",
    "agents:status": "node lib/orch-agents.mjs status",
    "agents:start": "node lib/orch-start-real.mjs",
    "agents:assign": "node lib/orch-agents.mjs assign",
    "agents:workflow": "node lib/orch-agents.mjs workflow"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "sequelize": "^6.35.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "vitest": "^1.0.0"
  }
}
